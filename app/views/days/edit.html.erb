<ol class="breadcrumb text-left">
  <li class="breadcrumb-item active"><%= link_to @itinerary.trip_title, itinerary_path(@itinerary) %></li>
  <li class="breadcrumb-item in-active">Edit Day</li>
</ol>

<div class="form-container text-center">
  <h2>Edit Day</h2>
  <%= form_for @day, url: itinerary_day_path(@itinerary, @day), method: :patch do |f| %>
    <%= render partial: 'form', locals: { f: f } %>
  <% end %>
</div>